function infiReload(innerPrefix) {
	reloadCheck('inputImage', innerPrefix, function(div) {
		new infiInputFile(div);
	});
}

function infiInputFile(div) {
	var self = this;
	this.root = div;
	this.prefix = 'infi';
	this.root.mHandler = this;
	this.innerInfo = utilGetInnerInfo(this.root);
	
